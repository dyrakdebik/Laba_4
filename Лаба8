#include <stdio.h>
#include <locale.h>


int main() {
	setlocale(LC_ALL, "Russian");
	int b, k, a, z, zad;
	printf("1-5 задача ");
	scanf("%d", &zad);
	if (zad == 1) {
		printf("Размер файла в байтах - ");
		scanf("%d", &b);
		k = b / 1024;
		printf("\nКоличество килобайт = %d", k);
	}
	else if (zad == 2) {
		printf("Число А - ");
		scanf("%d", &a);
		printf("Число B - ");
		scanf("%d", &b);
		k = a / b;
		printf("Количество отрезков с длиной В = %d", k);
	}
	else if (zad == 3) {
		printf("Число А - ");
		scanf("%d", &a);
		printf("Число B - ");
		scanf("%d", &b);
		k = a / b;
		z = a - (k * b);
		printf("Свободная длина = %d", z);
	}
	else if (zad == 4) {
		printf("Введите двухзначное число - ");
		scanf("%d", &a);
		z = a % 10;
		b = a / 10;
		k = z * 10 + b;
		printf("%d", k);
	}
	else if (zad == 5) {
		printf("Введите трехзначное число - ");
		scanf("%d", &a);
		b = a / 100;
		z = a % 100;
		k = z * 10 + b;
		printf("%d", k);
	}
	else if (zad > 5 % %zad < 1) {
		printf("Подумай еще раз");
	}

}
