int main() {
	setlocale(0, "");
	int zadanie;
	cout << "Введите номер задания - ";
	cin >> zadanie;
	if (zadanie == 1) {

	}
	else if (zadanie == 2) {
		;
		publication* pubPtr[100];// массив указателей на объекты класса publication
		int n = 0;// количество действительно занятых элементов массива
		char da;
		do {
			cout << "Вводим бумажную или аудиокнигу? (b/a) - ";
			cin >> da;
			if (da == 'b')
				pubPtr[n] = new book;
			else
				pubPtr[n] = new tape;
			cout << "Ввод данных издания " << n + 1 << " - " << endl;
			pubPtr[n++]->getdata();
			cout << "\nВвести следующее издание? (d/n) - ";
			cin >> da;
			cout << endl;
		} while (da == 'd');
		for (int j = 0; j < n; j++) {
			cout << "Данные издания " << j + 1 << ":";
			pubPtr[j]->putdata();
			if (pubPtr[j]->isOversize()) cout << "Превышение размера!\n"; // если издание слишком большого размера
		}
	}
	else if (zadanie == 3) {
		Distance dist1 = 3.5;
		Distance dist2 = 2.0;
		float Wdist;// для хранения результата
		// заданы два интервала в английских мерах длины, покажем их на экране
		cout << "dist1 = ";
		dist1.showdist(); // выведем на экран изначальные значения
		cout << "\ndist2 = ";
		dist2.showdist();
		// перемножим два заданных интервала, меняя сомножители местами,
		// выведем результаты на экран
		Wdist = dist1 * dist2;
		cout << "\ndist1 * dist2 = " << Wdist << " кв.футов";
		Wdist = dist2 * dist1;
		cout << "\ndist2 * dist1 = " << Wdist << " кв.футов";
		// тоже самое с вещественным числом
		Wdist = 10 * dist1;
		cout << "\n10 * dist1 = " << Wdist << " кв.футов";
		Wdist = dist2 * 10;
		cout << "\n10 * dist2 = " << Wdist << " кв.футов\n";
	}
	else if (zadanie == 4) {
		const int ASIZE = 10;// количество элементов в массиве
		Array arr1(ASIZE);
		for (int j = 0; j < ASIZE; j++)// заполним массив arr1 квадратами целых чисел
			arr1[j] = j * j;
		cout << "arr1 = ";
		arr1.display();
		cout << endl;
		// тестируем копирующий конструктор
		Array arr2(arr1);
		cout << "arr2 = ";
		arr2.display();
		cout << endl;
		// тестируем операцию присваивания массивов
		Array arr3(5), arr4(10), arr5(15);
		arr3 = arr1;
		arr4 = arr1;
		arr5 = arr1;
		cout << "arr3 = "; 
		arr3.display(); cout << endl;
		cout << "arr4 = "; 
		arr4.display(); cout << endl;
		cout << "arr5 = "; 
		arr5.display(); cout << endl;
		// тестируем присваивание самому себе
		arr1 = arr1;
		cout << "arr1 = ";
		arr1.display();
		cout << endl;
		// тестируем множественное присваивание
		Array arr6, arr7;
		arr7 = arr6 = arr1;
		cout << "arr6 = "; 
		arr6.display(); 
		cout << endl;
		cout << "arr7 = "; 
		arr7.display(); 
	}
	else if (zadanie == 5) {
		publication* pubPtr[100];// массив указателей на объекты класса publication
		int n = 0;// количество действительно занятых элементов массива
		char da;
		do {
			cout << "Вводим бумажную или аудиокнигу? (b/а): ";
			cin >> da;
			if (da == 'b')
				pubPtr[n] = new book;
			else
				pubPtr[n] = new tape;
			cout << "Ввод данных издания " << n + 1 << ":" << endl;
			pubPtr[n++]->getdata();
			cout << "\nВвести следующее издание? (d/n): ";
			cin >> da;
			cout << endl;
		} while (da == 'd');
		for (int j = 0; j < n; j++) {
			cout << "Данные издания " << j + 1 << ":";
			pubPtr[j]->putdata();
			// если издание слишком большого размера, выведем соответствующую пометку
			if (pubPtr[j]->isOversize()) cout << "Превышение размера!\n";
		}
	}
	else if (zadanie == 6) {
		wchar_t da;
		bMoney sum1, sum2, rez1;// для двух денежных сумм и результата операций
		long double n, rez2;// для вещественного числа и результата операций
		do {
			wcout << "1. "; sum1.getmoney();
			wcout << "2. "; sum2.getmoney();
			wcout << L"3. Введите вещественное число: "; wcin >> n; wcout << endl;
			// выполняем перегруженные операции и выводим результаты на экран
			rez1 = sum1 + sum2; 
			wcout << "1. sum1 + sum2 = "; 
			rez1.putmoney(); 
			wcout << endl;
			rez1 = sum1 - sum2; 
			wcout << "2. sum1 - sum2 = "; 
			rez1.putmoney(); 
			wcout << endl;
			rez1 = sum1 * n;  
			wcout << "3. sum1 * n  = "; 
			rez1.putmoney(); 
			wcout << endl;
			rez2 = sum1 / sum2; 
			wcout << "4. sum1 / sum2 = " << rez2 << endl;
			rez1 = sum1 / n;  
			wcout << "5. sum1 / n  = "; 
			rez1.putmoney(); 
			wcout << endl;
			rez1 = n * sum1;  
			wcout << "6. n * sum1  = "; 
			rez1.putmoney(); 
			wcout << endl;
			rez2 = n / sum1;   
			wcout << "7. n / sum1 = " << rez2 << endl << endl;
			wcout << L"Попробовать с другими исходными данными (d/n)? "; 
			wcin >> da; 
			wcout << endl;
		} while (da != 'n');
	}
	else if (zadanie == 7) {
		bMoney sum1, sum2;
		// тестируем округление «вниз»
		sum1 = L"$1,234,567.23";
		sum1.putmoney();
		wcout << " -> ";
		sum2 = round(sum1);
		sum2.putmoney();
		wcout << endl;
		// тестируем округление «вверх»
		sum1 = L"$1,234,567.50";
		sum1.putmoney(); 
		wcout << " -> ";
		sum2 = round(sum1);
		sum2.putmoney(); 
	}
	else if (zadanie == 8) {
		Stack s;// создадим стек
		Number n1(2.5), n2(3.55), n3(1.75), n4(2.0), n5(3.14159); // создадим объекты обоих видов
		Operator plus('+'), mult('*'), minus('-'), div('/');
		//поместим в стек адреса объектов обоих видов
		s.push(&n1); s.push(&plus); s.push(&n2);
		s.push(&mult); s.push(&n3); s.push(&plus);
		s.push(&n4); s.push(&div); s.push(&n5);
		while (s.gettop() > 0) {// пока стек не пуст будут извлекаться объекты обоих видов из стека и выводиться на экран в зависимости от вида объекта
			if (s.isNumber())
				cout << s.pop()->getNumber();
			else
				cout << s.pop()->getOperator();
			// если объект не последний в стеке, вывести пробел для разделения токенов на экране
			if (s.gettop() > 0) cout << ' ';
		}
	}
	else if (zadanie == 9) {
		srand(time(0));
		int total;
		cout << "Введите количество лошадей (от 1 до 7) - ";
		cin >> total;
		comHorse cH(total);
		cH.output_distance();
		cout << endl;
		for (int i = 0; i < total; i++) {
			cH.test_horse(i);
		}
	}
	else if (zadanie == 10) {
		linklist* list1 = new linklist;// создаем связный список
		list1->additem(11); // добавляем элементы в список
		list1->additem(22);
		list1->additem(33);
		// выведем список на экран
		cout << "list1 = ";
		list1->display();
		linklist list2(*list1);// тестируем перегруженный копирующий конструктор
		linklist list3;
		list3 = *list1;// тестируем перегруженный оператор присваивания
		delete list1; // удаляем первоначальный список
		// выводим тестовые списки на экран
		cout << "list2 = ";
		list2.display();
		cout << "list3 = ";
		list3.display();
	}
	else if (zadanie == 11) {
		char da;
		char string[LEN];// строка с арифметическим выражением для разбора
		cout << "Введите арифметическое выражение (2.5+3.0*4/3.148-2.84)";
		do {
			cout << "\nВведите выражение: ";// получаем строку от пользователя
			ws(cin);// предварительно убрав из потока cin ведущие пробельные символы
			cin.get(string, LEN);
			express* eptr = new express(string);// создаем объект-выражение для разбора
			eptr->parse();// выполняем разбор  выражения
			cout << "Результат вычисления выражения - " << eptr->solve();// получаем результат вычисления
			delete eptr;
			cout << "\nПродолжаем? (d/n)? ";
			cin >> da;
		} while (da == 'd');
	}
}
